# Privy Configuration
# Get these from: https://dashboard.privy.io/
# App ID is public (used in browser)
NEXT_PUBLIC_PRIVY_APP_ID=

# App Secret is private (used on server only - DO NOT expose to browser)
PRIVY_APP_SECRET=

# Vercel Postgres
# These will be automatically set by Vercel when you connect your database
# For local development, get these from your Vercel project settings
POSTGRES_URL=
POSTGRES_PRISMA_URL=
POSTGRES_URL_NON_POOLING=
POSTGRES_USER=
POSTGRES_HOST=
POSTGRES_PASSWORD=
POSTGRES_DATABASE=

